{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-classes-per-file": ["error", 2],
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "no-use-before-define": 0,
        "max-len": ["error", { "code": 120 }]

    }
}
